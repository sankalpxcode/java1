public class Test {
    public static void main(String[] args) {
        try {
            System.out.println("Welcome to Java");
            int sum = 10 / 0;
            System.out.println("2");
        } catch (ArithmeticException e) {
            System.out.println("3");
        } catch (Exception e) {
            System.out.println("4");
        } finally {
            System.out.println("5");
        }
    }
}
